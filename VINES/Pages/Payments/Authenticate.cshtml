@page
@model VINES.Pages.Payments.SuccessModel
@using Microsoft.AspNetCore.Authorization
@inject IAuthorizationService AuthorizationService
@{
}

@if (((await AuthorizationService.AuthorizeAsync(User, "AdvertiserOnly")).Succeeded) || ((await AuthorizationService.AuthorizeAsync(User, "PatientOnly")).Succeeded))
{
<!DOCTYPE html>

<html>

    <head>
        <meta name="viewport" content="width=device-width"/>
        <title>Success</title>
    </head>


    <body>

        <h3>Payment Successful</h3>
    </body>
</html>
}